<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sabapp">

    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/mahmanager"
        android:label="Mahmanager"
        android:roundIcon="@drawable/mahmanager"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".EditEventActivity" android:parentActivityName=".MainActivity"/>
        <activity android:name=".AddEventActivity"  android:parentActivityName=".MainActivity"/>
        <activity android:name=".EventActivity"  android:parentActivityName=".MainActivity"/>
        <activity android:name=".PhoneActivity"  />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>